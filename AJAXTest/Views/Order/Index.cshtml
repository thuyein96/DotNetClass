<head>
    <script src="https://code.jquery.com/jquery-3.7.0.js"></script>
</head>
<h1>This is Order Page.</h1>
<form>
    <p id="myP">This is Current Time.</p>
    <button id="btnShowTime" class="btn btn-primary">Show me Time</button>
    <p id="count">This is show count.</p>
    <button id="btnShowCount" class="btn btn-danger">Show Count</button>
</form>
<script>

    $("#btnShowTime").on('click', function(evt){
        evt.preventDefault();
        var url="/order/TellMeNow";
        //AJAX call to the server side
        $.get(url,null,function(data){
            //callback function tot the client side
            $("#myP").html(data);
        })
    });

    $("#btnShowCount").click(function(evt){
        evt.preventDefault();
        var url="/order/ShowCount";
        //AJAX call to the server side
        $.get(url,null,function(data){
            //callback function tot the client side
            $("#count").html(data);
        })
    })
</script>